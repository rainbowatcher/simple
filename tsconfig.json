{
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "include": ["packages/**/*.ts", "unocss.config.ts"]
}
